  "private": true,
  "scripts": {
    "postinstall": "npx jetify",
    "start": "react-native start",
    "lint": "eslint .",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.tsx --bundle-output android/app/src/main/assets/index.android.bundle",
    "build-android": "yarn bundle-android && cd android && ./gradlew assembleRelease",
    "bundle-ios": "react-native bundle --platform ios --dev false --entry-file index.tsx --bundle-output ios/main.jsbundle --assets-dest ios",
    "android": "react-native run-android",
    "clean": "cd android && ./gradlew clean",
    "reset-cache": "yarn start --resetCache"
  },
