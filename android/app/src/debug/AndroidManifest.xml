     android:usesCleartextTraffic="true"
     android:networkSecurityConfig="@xml/network_config"
     tools:replace="android:networkSecurityConfig"
     tools:targetApi="28"
     tools:ignore="GoogleAppIndexingWarning" />
</manifest>
